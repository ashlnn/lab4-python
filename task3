def split_list(lst, index):
    # Розбиває список на два за вказаним індексом
    first_part = lst[:index]
    second_part = lst[index:]
    return first_part, second_part

# Користувач вводить список
numbers = list(map(int, input("Введіть числа через пробіл: ").split()))

# Користувач вводить індекс для розбиття
split_index = int(input("Введіть номер елемента для розбиття: "))

# Викликаємо функцію
part1, part2 = split_list(numbers, split_index)

# Виводимо результати
print("Оригінальний список:", numbers)
print("Перша частина:", part1)
print("Друга частина:", part2)
